nav.navbar.navbar-default
    .navbar-header
        button(type='button', data-toggle='collapse', data-target='.navbar-collapse').navbar-toggle.collapsed
          span.sr-only
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='/') HOME
      
    .collapse.navbar-collapse.navbar-collapse
        ul.nav.navbar-nav
          li(class={active: title === 'Outage Dashboard'})
            a(href='/dashboard') Dashboard
          li(class={active: title === 'Report an Outage'})
            a(href='/new') Report Outage
        if process.env.AZREGION
          ul.nav.navbar-nav.navbar-right
            li
              span.navbar-text= `${process.env.AZREGION}`
